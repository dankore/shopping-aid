  <div id="category-wrapper" class="w-full border-b border-gray-700">
    <h2
      id="category-title"
      class="p-2 flex justify-between cursor-pointer rounded-tr rounded-tl shadow-2xl text-white background-color"
    >
      FRUITS
      <span class="goThruThisElemForEvents">&#65310;</span>
    </h2>
    <div
      class="p-4 rounded-br rounded-bl bg-white swing-in-top-fwd"
      id="checkboxes-wrapper-fruits"
      style="display: none;"
    >
      <%if(fruits.length > 0){%> <% fruits.forEach(item => { %> <%-
      include("checkboxAndCounter", { item : item } )%> <% }) %> <%}%>
      <span class="checkbox-select-item hidden"></span>
    </div>
  </div>
